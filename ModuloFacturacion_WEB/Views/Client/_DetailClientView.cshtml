@model FactClient

<div class="modal fade" id="DetailClient-@Model.CliIdentification" aria-labelledby="DetailClientLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="DetailClientLabel">Detalles del CLIENTE</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <dl class="row">
                    <dt class="col-sm-3">
                        Cédula
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.CliIdentification)
                    </dd>
                    <dt class="col-sm-3">
                        Nombre
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.CliName)
                    </dd>
                    <dt class="col-sm-3">
                        Fecha
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.CliBirthday)
                    </dd>
                    <dt class="col-sm-3">
                        Dirección
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.CliAddres)
                    </dd>
                    <dt class="col-sm-3">
                        Celular
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.CliPhone)
                    </dd>
                    <dt class="col-sm-3">
                        Correo
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.CliMail)
                    </dd>
                    <dt class="col-sm-3">
                        Estado
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.CliStatus)
                    </dd>
                    <dt class="col-sm-3">
                        Tipo
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.TypId)
                    </dd>
                </dl>
            </div>

        </div>
    </div>
</div>